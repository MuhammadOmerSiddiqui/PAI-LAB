    "import cv2\n",
    "import numpy as np\n",
    "image = np.zeros((300,400,3),dtype=np.uint8)\n",
    "cv2.rectangle(image,(50,50),(200,150),(0,0,255),-1)\n",
    "cv2.circle(image,(300,200),50,(0,255,0),-1)\n",
    "cv2.imshow(\"Rectangle and Circle\",image)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
  }
